const description = "Conoce el paso a paso del mantenimiento que le hacemos a la maquinaria";
---

<Section id="process">
  <div class="mt-[140px]">
    <SectionTitle
      sectionTitle="El proceso de nuestro trabajo"
      description={description}
    />
    <Accordion />
  </div>
</Section>
